<DemoContainer>
    <ApexChart TItem="SupportIncident" Debug>
      

        <ApexPointSeries TItem="SupportIncident"
                         Items="incidents.OrderBy(e=>e.WeekNumber)"
                         SeriesType="SeriesType.Scatter"
                         Name="Scatter"
                         XValue="@(e => e.WeekNumber)"
                         YAggregate="@(e => (decimal)e.Min(e=> e.Severity) -5)"
                         Color="#FF0000"  />

                           <ApexBoxPlotSeries TItem="SupportIncident"
                           Items="incidents.OrderBy(e=>e.WeekNumber)"
                           Name="Box"
                           XValue="@(e => e.WeekNumber)"
                           YValue="@(e => e.Severity)" />
    </ApexChart>
</DemoContainer>

@code {
    private List<SupportIncident> incidents { get; set; } = SampleData.GetSupportIncidents(30, 60);
    }
